<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
	<LinearLayout 
        android:id="@+id/ll_showcompanyname"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/transparent"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >
		<ImageView 
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_qiye"/>
        <TextView 
        	android:id="@+id/tv_showcompanyname"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置企业名称"/>
     </LinearLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
	<LinearLayout 
        android:id="@+id/ll_showindustry"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/transparent"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >
		<ImageView 
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_hangye"/>
        <TextView 
            android:id="@+id/tv_showindustry"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置所属行业"/>
     </LinearLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
	<LinearLayout 
        android:id="@+id/ll_showcompanycity"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/transparent"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >
		<ImageView 
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_didian"/>
        <TextView 
            android:id="@+id/tv_showcompanycity"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置企业所在地"/>
     </LinearLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
     <RelativeLayout 
        android:id="@+id/rl_showproductlist"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
    	android:background="@drawable/selector_item" >
		<ImageView 
		    android:id="@+id/iv_one"
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_hangye"/>
        <TextView 
            android:id="@+id/tv_showproductlist"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/iv_pro_arrow"
	        android:layout_toRightOf="@+id/iv_one"
		    android:layout_centerVertical="true"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置主营产品"/>
        <ImageView 
		    android:id="@+id/iv_pro_arrow"
		    android:layout_centerVertical="true"
		    android:layout_alignParentRight="true"
            style="@style/right_arrows_style"/>
     </RelativeLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
     <LinearLayout 
        android:id="@+id/ll_showposition"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/transparent"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >
		<ImageView 
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_zhiye"/>
        <TextView 
            android:id="@+id/tv_showposition"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置所在职位"/>
     </LinearLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
     <LinearLayout 
        android:id="@+id/ll_showcompanyweb"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/transparent"
	    android:gravity="center_vertical"
	    android:orientation="horizontal" >
		<ImageView 
		    android:layout_width="30dp"
		    android:layout_height="30dp"
		    android:layout_marginLeft="10dp"
		    android:src="@drawable/show_wangzhan"/>
        <TextView 
            android:id="@+id/tv_showcompanyweb"
            android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        style="@style/user_setting_name"
	        android:textSize="16sp"
	        android:text="未设置企业网站"/>
     </LinearLayout>
     <View
        android:layout_width="match_parent"
        android:layout_height="0.5dip"
        android:background="@color/color_xiahuaxian" />
</LinearLayout>